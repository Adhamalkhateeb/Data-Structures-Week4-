using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp2
{
    class LinierSearch
    {
        static public int Search(int[] arr,int Element)
        {
            for(int i = 0; i < arr.Length; i++)
            {
                if(arr[i] == Element)
                { 
                    return i;
                }
            }
            return -1;
        }
    }
}
